<br><br><br>

<div class="panel panel-primary col-md-9">
  <div class="panel-heading">
    <h1 class="panel-title">Add a job</h1>
  </div>
  <div class="panel-body">
    <%= simple_form_for [:admin,@job] do |f| %>
<%= f.input :title %>
<%= f.input :brand %>
<%= f.input :description %>
<%= f.input :wage_lower_bound, :label => "薪资下限" %>
<%= f.input :wage_upper_bound, :label => "薪资上限" %>
<%= f.input :location %>
<%= f.input :contact_email %>
<%= f.input :is_hidden %>
<%= f.submit "Submit" %>
<% end %>
  </div>
</div>
